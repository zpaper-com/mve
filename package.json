{
  "name": "mve-backend",
  "version": "1.0.0",
  "description": "MVE Project Backend - PDF workflow management system",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:seed": "tsx prisma/seed.ts",
    "db:setup": "tsx scripts/setup-database.ts --seed",
    "db:setup:force": "tsx scripts/setup-database.ts --force --seed",
    "db:verify": "tsx scripts/setup-database.ts --verify-only",
    "db:backup": "tsx scripts/setup-database.ts --skip-backup",
    "db:test": "tsx scripts/test-database.ts",
    "cdk": "cdk",
    "cdk:bootstrap": "cdk bootstrap --region us-east-1",
    "cdk:diff": "cdk diff --all",
    "cdk:synth": "cdk synth --all",
    "cdk:deploy": "cdk deploy --all --require-approval never",
    "cdk:deploy:networking": "cdk deploy MveNetworkingStack --require-approval never",
    "cdk:deploy:data": "cdk deploy MveDataStack --require-approval never",
    "cdk:deploy:storage": "cdk deploy MveStorageStack --require-approval never",
    "cdk:deploy:compute": "cdk deploy MveComputeStack --require-approval never",
    "cdk:destroy": "cdk destroy --all"
  },
  "keywords": [
    "pdf",
    "workflow",
    "express",
    "typescript",
    "socketio"
  ],
  "author": "MVE Team",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.468.0",
    "@aws-sdk/s3-request-presigner": "^3.468.0",
    "@prisma/client": "^5.7.1",
    "auth0": "^4.28.0",
    "axios": "^1.11.0",
    "base32-encode": "^2.0.0",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dompurify": "^3.0.8",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsdom": "^24.0.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "nodemailer": "^6.9.7",
    "pdf-lib": "^1.17.1",
    "rate-limiter-flexible": "^4.0.1",
    "redis": "^4.6.12",
    "sanitize-html": "^2.12.1",
    "socket.io": "^4.7.4",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/csurf": "^1.11.5",
    "@types/dompurify": "^3.0.5",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/jest": "^29.5.8",
    "@types/jsdom": "^21.1.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^6.4.14",
    "@types/sanitize-html": "^2.11.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "aws-cdk": "^2.118.0",
    "aws-cdk-lib": "^2.118.0",
    "constructs": "^10.0.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prisma": "^5.7.1",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
